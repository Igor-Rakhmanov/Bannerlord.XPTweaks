  <ModuleType value="Community" />
  <DependedModules>
    <DependedModule Id="Bannerlord.Harmony" DependentVersion="v2.2.2"/>
	<DependedModule Id="Bannerlord.MBOptionScreen" DependentVersion="v5.3.1"/>

	<DependedModule Id="Native" DependentVersion="v1.0.1"/>
    <DependedModule Id="SandBoxCore" DependentVersion="v1.0.1"/>
    <DependedModule Id="Sandbox" DependentVersion="v1.0.1"/>
    <DependedModule Id="StoryMode" DependentVersion="v1.0.1"/>
    <DependedModule Id="CustomBattle" DependentVersion="v1.0.1"/>
  </DependedModules>
  <!-- Community Dependency Metadata -->
  <!-- https://github.com/BUTR/Bannerlord.BUTRLoader#for-modders -->
  <DependedModuleMetadatas>
    <DependedModuleMetadata id="Bannerlord.Harmony" order="LoadBeforeThis" version="v2.2.2.*"/>
	<DependedModuleMetadata Id="Bannerlord.MBOptionScreen" order="LoadBeforeThis" version="v5.3.1.*"/>

	<DependedModuleMetadata id="Native" order="LoadBeforeThis" version="v1.0.*"/>
    <DependedModuleMetadata id="SandBoxCore" order="LoadBeforeThis" version="v1.0.*"/>
    <DependedModuleMetadata id="Sandbox" order="LoadBeforeThis" version="v1.0.*"/>
    <DependedModuleMetadata id="StoryMode" order="LoadBeforeThis" version="v1.0.*"/>
    <DependedModuleMetadata id="CustomBattle" order="LoadBeforeThis" version="v1.0.*"/>
  </DependedModuleMetadatas>
  <!-- Community Dependency Metadata -->
  <SubModules>
    <SubModule>
      <Name value="$modulename$" />
      <DLLName value="$modulename$.dll" />
      <SubModuleClassType value="$modulename$.SubModule" />
      <Tags />
    </SubModule>
  </SubModules>
</Module>